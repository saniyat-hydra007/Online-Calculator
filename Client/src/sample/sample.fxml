<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="434.0" prefWidth="481.0">
         <children>
            <TextField fx:id="inputA" layoutX="33.0" layoutY="20.0" prefHeight="86.0" prefWidth="193.0">
               <font>
                  <Font size="24.0" />
               </font>
            </TextField>
            <TextArea fx:id="resultTA" editable="false" layoutX="33.0" layoutY="117.0" prefHeight="144.0" prefWidth="415.0">
               <font>
                  <Font size="24.0" />
               </font>
            </TextArea>
            <Button layoutX="33.0" layoutY="278.0" mnemonicParsing="false" onAction="#addFunction" prefHeight="44.0" prefWidth="79.0" text="+">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button layoutX="154.0" layoutY="278.0" mnemonicParsing="false" onAction="#subFunction" prefHeight="44.0" prefWidth="79.0" text="-">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button layoutX="262.0" layoutY="278.0" mnemonicParsing="false" onAction="#multFunction" prefHeight="44.0" prefWidth="79.0" text="-">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button layoutX="369.0" layoutY="278.0" mnemonicParsing="false" onAction="#divFunction" prefHeight="44.0" prefWidth="79.0" text="/">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button layoutX="204.0" layoutY="357.0" mnemonicParsing="false" onAction="#HistoryFunction" prefHeight="44.0" prefWidth="79.0" text="History">
               <font>
                  <Font name="Cambria" size="14.0" />
               </font>
            </Button>
            <TextField fx:id="inputB" layoutX="255.0" layoutY="20.0" prefHeight="86.0" prefWidth="193.0">
               <font>
                  <Font size="24.0" />
               </font>
            </TextField>
         </children>
         <effect>
            <Glow />
         </effect>
      </AnchorPane>
   </children>
</GridPane>
